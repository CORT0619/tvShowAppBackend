{
	"compilerOptions": {
		"module": "Node16",
		"sourceMap": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"noImplicitAny": false,
		"declaration": true,
		"moduleResolution": "Node16",
		"esModuleInterop": true,
		"types": [
			"jest",
			"bcrypt",
			"cookie-parser",
			"jsonwebtoken",
			"multer",
			"node",
			"express"
		]
	},
	"include": [
		"src/**/*",
		"src/test/*"
	]
}